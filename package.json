{
  "name": "aws-sessions-switcher",
  "version": "0.2.1",
  "description": "A tool to help switching between multiple AWS environments easy and seamless",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aws-sessions-switcher": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "start": "node dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEV-BALAN-JAYAVICTOR/aws-sessions-switcher.git"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "keywords": [
    "aws",
    "authentication",
    "cli",
    "sessions",
    "profiles"
  ],
  "author": "Balan Jayavictor",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.1",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "semantic-release": "19.0.5",
    "ts-jest": "^29.3.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.782.0",
    "@inquirer/prompts": "^7.4.1",
    "@types/luxon": "^3.6.2",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "configparser": "^0.3.10",
    "configstore": "^7.0.0",
    "inquirer": "12.5.2",
    "luxon": "^3.6.1"
  }
}
